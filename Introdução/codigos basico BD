REATE DATABASE AulaDataPR;
USE AulaDataPR;

CREATE TABLE usuario(
       id INT AUTO_INCREMENT PRIMARY KEY,
       username VARCHAR(50) NOT NULL,
       password VARCHAR(20) NOT NULL
);
